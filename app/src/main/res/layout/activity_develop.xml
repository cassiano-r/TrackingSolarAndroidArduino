<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="cassiano.trackingsolarandroidarduino.Develop">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerHorizontal="true"
        android:layout_alignParentTop="true">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Develop:"
            android:id="@+id/textView4"
            android:textSize="100px" />

        <HorizontalScrollView
            android:layout_width="match_parent"
            android:layout_height="28dp"
            android:id="@+id/horizontalScrollView3"
            android:layout_gravity="right" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Cassiano Ricardo Gomes da Silva"
            android:id="@+id/textView6" />

        <HorizontalScrollView
            android:layout_width="match_parent"
            android:layout_height="34dp"
            android:id="@+id/horizontalScrollView4"
            android:layout_gravity="right" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Marcus Vinicius Gomes da Silva"
            android:id="@+id/textView7" />

        <HorizontalScrollView
            android:layout_width="match_parent"
            android:layout_height="34dp"
            android:id="@+id/horizontalScrollView5"
            android:layout_gravity="right" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Rafael Massahiro Taira Kawasaki"
            android:id="@+id/textView8" />
    </LinearLayout>
</RelativeLayout>
